    "@material-ui/icons": "^4.0.0",
    "@material-ui/styles": "^4.0.0",
    "axios": "^0.19.0",
    "axios-progress-bar": "^1.2.0",
    "core-js": "^2.5.1",
    "dateformat": "^3.0.3",
    "jest-dom": "^3.2.2",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "node-sass": "^4.12.0",
    "postcss-import": "^12.0.1",
    "prop-types": "^15.7.2",
    "ramda": "^0.26.1",
    "react": "^16.8.6",
    "react-breadcrumbs-dynamic": "^1.2.0",
    "react-datepicker": "^1.5.0",
    "react-dom": "^16.8.6",
    "react-helmet": "^6.0.0-beta",
    "react-icons": "^3.3.0",
    "react-media": "^1.6.1",
    "react-modal": "^3.4.4",
    "react-moment": "^0.9.2",
    "react-resize-detector": "^4.1.3",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^3.0.1",
    "react-select": "^2.4.3",
    "react-table": "^6.8.0",
    "react-test-renderer": "^16.8.6",
    "react-testing-library": "^7.0.1",
    "react-through": "^1.1.3",
    "styled-components": "^2.2.1"
  },
  "scripts": {
    "tailwind:css": "tailwind build src/css/tailwind.src.pcss -c tailwind.js -o src/css/tailwind.css",
    "start": "npm-run-all --parallel watch:css watch:component-css start:react",
    "build": "npm-run-all build:css build:react",
    "build:css": "postcss src/css/tailwind.src.pcss -o src/css/tailwind.css",
    "watch:css": "postcss src/css/tailwind.src.pcss -o src/css/tailwind.css -w",
    "watch:component-css": "postcss src/components/**/*.pcss --base src --dir src --ext module.css -w",
    "start:react": "HTTPS=true&&react-scripts start",
    "build:react": "react-scripts build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test": "react-scripts test",
    "e2e": "cd tests/e2e; gauge run specs; cd -",
    "coverage": "npm t -- --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@storybook/addon-actions": "^5.1.1",
    "@storybook/addon-centered": "^5.1.1",
    "@storybook/addon-info": "^5.1.1",
    "@storybook/addon-links": "^5.1.1",
    "@storybook/addons": "^5.1.1",
    "@storybook/react": "^5.1.1",
    "autoprefixer": "^9.5.1",
    "babel-loader": "8.0.5",
    "chai": "^4.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.2",
    "galenframework": "^2.4.4",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.2",
    "postcss-modules": "^1.4.1",
    "sinon": "^7.3.2",
    "tailwindcss": "^1.0.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
