  "scripts": {
    "tailwind:css": "tailwind build src/css/tailwind.src.pcss -c tailwind.js -o src/css/tailwind.css",
    "start": "npm-run-all --parallel watch:css watch:component-css start:react",
    "build": "npm-run-all build:css build:react build:component-css",
    "build:component-css": "postcss src/components/**/*.pcss --base src --dir src --ext module.css",
    "build:css": "tailwind build src/css/tailwind.src.pcss -c tailwind.js -o src/css/tailwind.css",
    "watch:css": "tailwind build src/css/tailwind.src.pcss -c tailwind.js -o src/css/tailwind.css -w",
    "watch:component-css": "postcss src/components/**/*.pcss --base src --dir src --ext module.css -w",
    "watch:common-css": "postcss src/css/common.src.pcss -o src/css/common.css -w",
    "start:react": "HTTPS=true&&react-scripts start",
    "build:react": "react-scripts build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test": "react-scripts test",
    "e2e": "cd tests/e2e; gauge run specs; cd -",
    "coverage": "npm t -- --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "extract:messages": "react-intl-cra 'src/**/*.{js,jsx}' -o 'src/i18n/messages/messages.json'",
    "manage:translations": "node ./translationRunner.js",
    "get-translations": "node ./getTranslations.js",
    "docs": "jsdoc -c ./jsdoc.json",
    "prettier": "npx prettier --write src/**/*.js"
  },
