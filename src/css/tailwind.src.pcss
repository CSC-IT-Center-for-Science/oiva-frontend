@import "tailwindcss/base";
@import "tailwindcss/components";
@import "tailwindcss/utilities";

h1 {
    @apply text-2xl;
    @apply font-bold;
}
h2 {
    @apply text-xl;
    @apply font-bold;
    margin-bottom: 0.83em;
}
h3 {
    @apply text-lg;
    @apply font-bold;
    margin-bottom: 0.83em;
}
h4 {
    @apply text-base;
    @apply font-bold;
    margin-bottom: 0.83em;
}
